<launch>
  <arg default="$(find manipulator_description)/urdf/manipulator.xacro" name="model"/>
  <arg default="true" name="gui"/>
  <arg default="$(find manipulator_description)/launch/urdf.rviz" name="rvizconfig"/>
  <param command="$(find xacro)/xacro $(arg model)" name="robot_description"/>
  <param name="use_gui" value="$(arg gui)"/>
<!--
  <node name="joint_state_publisher_gui" pkg="joint_state_publisher_gui" type="joint_state_publisher_gui"/>
-->
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>
  <node args="-d $(arg rvizconfig)" name="rviz" pkg="rviz" required="true" type="rviz"/>

<rosparam file="$(find kamitact_manipulator)/launch/controller.yaml" command="load"/>
<node args="Rev2_position_controller Rev4_position_controller Rev6_position_controller Rev8_position_controller Rev10_position_controller Rev11_position_controller Rev12_position_controller Rev13_position_controller Rev14_position_controller Rev16_position_controller joint_state_controller " name="controller_spawner" ns="manipulator" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>


<node name="manipulator_description_controller_node" output="screen" pkg="manipulator_description" type="manipulator_description_controller_node">
  <remap from="manipulator_description_controller_node/publisher" to="/joint_states"/>
<!--
-->
</node>

</launch>
